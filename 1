import matplotlib.pyplot as plt
filename = 'DATA.txt'
X,Y,Z = [],[],[]
with open(filename, 'r') as f:#1
    lines = f.readlines()#2
    for line in lines:#3
        value = [float(s) for s in line.split()]#4
        X.append(value[0])#5
        Y.append(value[1])
        Z.append(value[2])

print(X)
print(Y)
print(Z)

plt.plot(X, Y,Z)
plt.show()
